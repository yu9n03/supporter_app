<% @users.each do |user| %>
  <div class="show-records">
    <span class="show-date"><%= user.id %></span>
    <span class="show-weight"><%= user.nickname %>  kg</span>
    <%= link_to "MyPageへ" ,mypage_path(user.id), class: :show_edit_link %>
    <%= link_to "削除" ,user_registration_path(user.id),method: :delete, class: :show_edit_link %>
  </div>
<% end %>